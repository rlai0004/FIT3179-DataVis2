{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A vertical box plot showing median and lower and upper quartiles of the distribution of body mass of penguins.",
    "data": {
        "url": "data/2023_rankings.csv"
    },
    "transform": [
        {
            "filter": "datum.rank_min <= 1501"}
    ],
    "mark": "boxplot",
    "encoding": {
      "x": {"field": "region", "type": "nominal"},
      "color": {"field": "region", "type": "nominal", "legend": null},
      "y": {
        "field": "rank_min",
        "type": "quantitative"
      }
    }
  }
  